{
    "name": "capynews-server",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "prisma:migrate": "prisma migrate dev --name init",
        "prisma:generate": "prisma generate",
        "prisma:seed": "ts-node src/prisma/seed.ts"
    },
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "@prisma/client": "^5.8.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17",
        "@types/node": "^20.11.0",
        "typescript": "^5.3.3",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "prisma": "^5.8.0"
    }
}